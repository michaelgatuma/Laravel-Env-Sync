{
  "name": "jtant/laravel-env-sync",
  "description": "A package that makes sure that your .env file is in sync with your .env.example",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Jtant\\LaravelEnvSync\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jtant\\LaravelEnvSync\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4",
    "vlucas/phpdotenv": "^5.3",
    "illuminate/console": "^8.0",
    "illuminate/support": "^8.0",
    "illuminate/events": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4|^9.0",
    "mikey179/vfsstream": "^1.6",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^6.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Julien Tant",
      "email": "julien@craftyx.fr"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Jtant\\LaravelEnvSync\\EnvSyncServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
